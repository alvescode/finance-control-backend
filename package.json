{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "services:up": "sudo docker compose -f ./src/infra/compose.yaml up -d",
    "services:down": "sudo docker compose -f ./src/infra/compose.yaml down",
    "services:stop": "sudo docker compose -f ./src/infra/compose.yaml stop",
    "dev": "npm run services:up && npm run build && npm run start"
  },
  "author": "alvescode",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.19.2",
    "mysql2": "^3.10.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.3"
  }
}
